# Porta que o servidor vai escutar
PORT=3000

# Chave secreta usada para assinar tokens JWT
JWT_SECRET=your_jwt_secret_here

# Número de rounds usados pelo bcrypt para hash de senhas
SALT_ROUNDS=10

# URL de conexão com o banco de dados PostgreSQL
# Formato: postgres://username:password@host:port/database
DBSTORAGE=postgres://user:password@localhost:5432/database_name
